import java.util.Scanner;

public class FiboSeries {

	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		int a = 0, b = 1, c;
		
		int no = s.nextInt();
		
		System.out.print(a+" "+b);
		
		for(int i= 2; i<=no; i++)
		{
			c = a + b;
			System.out.println(" "+c);
			a = b;
			b = c;
		}
		s.close();
		                                                                                                                                             
	}
}
